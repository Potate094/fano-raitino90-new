{
  // Use official GitHub Actions schema for workflow files to reduce editor YAML warnings
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*"
  }
  ,
  // Disable YAML validation in workspace to avoid false-positive diagnostics from editors
  "yaml.validate": false,
  "yaml.lint": {
    "enable": false
  }
}
